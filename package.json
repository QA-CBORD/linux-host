  "homepage": "",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "start-ip": "ionic serve --address 10.28.160.60",
    "build": "npm run test && ng build",
    "test": "npm run lint && jest --detectOpenHandles",
    "test:ci": "jest --detectOpenHandles --verbose  --maxWorkers=4 --config jest.config.ci.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build-prod": "npm run test && ng build --configuration production",
    "build-appflow": "npm run test:ci && ng build --configuration production",
    "lint": "ng lint app --quiet",
    "e2e": "ng e2e",
    "i-patch": "./IncrementVersionNumber.sh",
    "i-minor": "./IncrementVersionNumber.sh -m",
    "i-major": "./IncrementVersionNumber.sh -p",
    "ionic-e2e:build:ios": "bash scripts/build-ios.sh",
    "ionic-e2e:build:android": "bash scripts/build-android.sh",
    "ionic-e2e:develop": "cross-env TS_NODE_PROJECT=tests/tsconfig.json  wdio tests/config/wdio.web.config.ts --watch",
    "ionic-e2e:run:web": "cross-env TS_NODE_PROJECT=\"tests/tsconfig.json\"  wdio tests/config/wdio.web.config.ts",
    "ionic-e2e:run:ios": "cross-env TS_NODE_PROJECT=tests/tsconfig.json  wdio tests/config/wdio.ios.config.ts --suite $SUITE",
    "ionic-e2e:run:android": "cross-env TS_NODE_PROJECT=\"tests/tsconfig.json\"  wdio tests/config/wdio.android.config.ts --suite $SUITE"
  },
  "pre-push": "lint",
