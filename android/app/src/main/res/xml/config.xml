<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="IonicNativeAuth">
    <param name="android-package" value="com.ionicframework.auth.IonicNativeAuth"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="DevicePlugin">
    <param name="android-package" value="com.ionicframework.IdentityVault.DevicePlugin"/>
  </feature>
  <feature name="VaultPlugin">
    <param name="android-package" value="com.ionicframework.IdentityVault.VaultPlugin"/>
    <param name="onload" value="true"/>
  </feature>
  <feature name="IdentityVaultPlugin">
    <param name="android-package" value="com.ionicframework.IdentityVault.IdentityVaultPlugin"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Permissions">
    <param name="android-package" value="com.android.plugins.Permissions"/>
  </feature>

  <feature name="Chooser">
    <param name="android-package" value="com.cyph.cordova.Chooser"/>
  </feature>

  <feature name="InAppBrowser">
    <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser"/>
  </feature>

  
  <preference name="ScrollEnabled" value="true" />
  <preference name="android-minSdkVersion" value="19" />
  <preference name="BackupWebStorage" value="none" />
  <preference name="SplashMaintainAspectRatio" value="true" />
  <preference name="FadeSplashScreenDuration" value="300" />
  <preference name="SplashShowOnlyFirstTime" value="false" />
  <preference name="SplashScreen" value="screen" />
  <preference name="SplashScreenDelay" value="3000" />
</widget>